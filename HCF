def function(a,b):
  factorsofa= [x for x in range(1,a+1) if a%x ==0]
  factorsofb= [x for x in range(1,b+1) if b%x ==0]
  common=[x for x in factorsofb if x in factorsofa]
  return (max(common))
print(function(10,15))

Efficient one:

def function(a,b):
  for x in range(a,0,-1):
    if b%x ==0 and a%x ==0:
      return x 
print(function(100,120))
